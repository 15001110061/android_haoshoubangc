<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_signet_two"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/common_white" >

    <include
        android:id="@+id/include_stub_title"
        layout="@layout/stub_title" />

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@id/include_stub_title" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical" >

            <RelativeLayout
                android:id="@+id/relativeLayout1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/include_stub_title"
                android:layout_marginTop="@dimen/dd_dimen_44px"
                android:background="@color/common_white" >

                <RelativeLayout
                    android:id="@+id/layout_login"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/dd_dimen_100px"
                    android:background="@color/common_white"
                    android:gravity="center" >

                    <ImageView
                        android:id="@+id/imageView1"
                        android:layout_width="@dimen/dd_dimen_100px"
                        android:layout_height="@dimen/dd_dimen_100px"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/icon_sig_three" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="30dp"
                        android:layout_toRightOf="@+id/imageView1"
                        android:gravity="center"
                        android:text="完善个人信息"
                        android:textColor="@color/common_yellow"
                        android:textSize="@dimen/dd_dimen_36px" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/layout_username"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/dd_dimen_80px"
                    android:layout_below="@id/layout_login"
                    android:layout_marginLeft="@dimen/dd_dimen_37px"
                    android:layout_marginRight="@dimen/dd_dimen_37px"
                    android:layout_marginTop="@dimen/dd_dimen_26px"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/textView1"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_gravity="center_vertical"
                        android:gravity="center"
                        android:text="居住地"
                        android:textColor="@color/common_text"
                        android:textSize="@dimen/dd_dimen_36px" />

                    <AutoCompleteTextView
                        android:id="@+id/et_signed_home"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="@dimen/dd_dimen_36px"
                        android:layout_toRightOf="@+id/textView1"
                        android:background="@null"
                        android:completionThreshold="1"
                        android:dropDownVerticalOffset="1.0dip"
                        android:ems="10"
                        android:gravity="center_vertical"
                        android:hint="输入家的位置"
                        android:imeOptions="actionSearch"
                        android:inputType="text|textAutoComplete"
                        android:maxLength="20"
                        android:scaleType="centerInside"
                        android:singleLine="true"
                        android:textColor="#707070"
                        android:textColorHint="#707070"
                        android:textSize="15dp" />
                </RelativeLayout>

                <View
                    android:id="@+id/layout_line"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/dd_dimen_1px"
                    android:layout_below="@+id/layout_username"
                    android:layout_marginLeft="@dimen/dd_dimen_37px"
                    android:layout_marginRight="@dimen/dd_dimen_37px"
                    android:layout_marginTop="@dimen/dd_dimen_20px"
                    android:background="@color/common_CCCCCC" />

                <RelativeLayout
                    android:id="@+id/ll_layout"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/dd_dimen_80px"
                    android:layout_below="@+id/layout_username"
                    android:layout_marginLeft="@dimen/dd_dimen_37px"
                    android:layout_marginRight="@dimen/dd_dimen_37px"
                    android:layout_marginTop="@dimen/dd_dimen_46px"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_gravity="center_vertical"
                        android:gravity="center"
                        android:text="是否有过培训经历"
                        android:textColor="@color/common_text"
                        android:textSize="@dimen/dd_dimen_36px" />

                    <CheckBox
                        android:id="@+id/cb_seting_training"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:background="@drawable/switch_selecor"
                        android:button="@null"
                        android:gravity="center_vertical" />
                </RelativeLayout>

                <View
                    android:id="@+id/layout_line2"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/dd_dimen_1px"
                    android:layout_below="@+id/ll_layout"
                    android:layout_marginLeft="@dimen/dd_dimen_37px"
                    android:layout_marginRight="@dimen/dd_dimen_37px"
                    android:layout_marginTop="@dimen/dd_dimen_20px"
                    android:background="@color/common_CCCCCC" />

                <LinearLayout
                    android:id="@+id/ll_layout3"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/dd_dimen_80px"
                    android:layout_below="@+id/layout_line2"
                    android:layout_marginLeft="@dimen/dd_dimen_37px"
                    android:layout_marginRight="@dimen/dd_dimen_37px"
                    android:layout_marginTop="@dimen/dd_dimen_26px"
                    android:orientation="horizontal"
                    android:visibility="gone" >

                    <TextView
                        android:id="@+id/et_signed_starttime"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_gravity="center_vertical"
                        android:background="@null"
                        android:gravity="center_vertical"
                        android:hint="开始时间"
                        android:singleLine="true"
                        android:textColor="@color/common_text"
                        android:textSize="@dimen/dd_dimen_36px" />
                </LinearLayout>

                <View
                    android:id="@+id/layout_line6"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/dd_dimen_1px"
                    android:layout_below="@+id/ll_layout3"
                    android:layout_marginLeft="@dimen/dd_dimen_37px"
                    android:layout_marginRight="@dimen/dd_dimen_37px"
                    android:layout_marginTop="@dimen/dd_dimen_20px"
                    android:background="@color/common_CCCCCC"
                    android:visibility="gone" />

                <LinearLayout
                    android:id="@+id/ll_layout7"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/dd_dimen_80px"
                    android:layout_below="@+id/layout_line6"
                    android:layout_marginLeft="@dimen/dd_dimen_37px"
                    android:layout_marginRight="@dimen/dd_dimen_37px"
                    android:layout_marginTop="@dimen/dd_dimen_26px"
                    android:orientation="horizontal"
                    android:visibility="gone" >

                    <TextView
                        android:id="@+id/et_signed_endtime"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_gravity="center_vertical"
                        android:background="@null"
                        android:gravity="center_vertical"
                        android:hint="结束时间"
                        android:singleLine="true"
                        android:textColor="@color/common_text"
                        android:textSize="@dimen/dd_dimen_36px" />
                </LinearLayout>

                <View
                    android:id="@+id/layout_line8"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/dd_dimen_1px"
                    android:layout_below="@+id/ll_layout7"
                    android:layout_marginLeft="@dimen/dd_dimen_37px"
                    android:layout_marginRight="@dimen/dd_dimen_37px"
                    android:layout_marginTop="@dimen/dd_dimen_20px"
                    android:background="@color/common_CCCCCC"
                    android:visibility="gone" />

                <RelativeLayout
                    android:id="@+id/ll_layout9"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/dd_dimen_80px"
                    android:layout_below="@+id/layout_line8"
                    android:layout_marginLeft="@dimen/dd_dimen_37px"
                    android:layout_marginRight="@dimen/dd_dimen_37px"
                    android:layout_marginTop="@dimen/dd_dimen_26px"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_gravity="center_vertical"
                        android:gravity="center"
                        android:text="是否有过签约经历"
                        android:textColor="@color/common_text"
                        android:textSize="@dimen/dd_dimen_36px" />

                    <CheckBox
                        android:id="@+id/et_signed_sign"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:background="@drawable/switch_selecor"
                        android:button="@null"
                        android:gravity="center_vertical" />
                </RelativeLayout>

                <View
                    android:id="@+id/layout_line4"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/dd_dimen_1px"
                    android:layout_below="@+id/ll_layout9"
                    android:layout_marginLeft="@dimen/dd_dimen_37px"
                    android:layout_marginRight="@dimen/dd_dimen_37px"
                    android:layout_marginTop="@dimen/dd_dimen_20px"
                    android:background="@color/common_CCCCCC" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/dd_dimen_1px"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/ll_layout"
                    android:layout_marginLeft="@dimen/dd_dimen_37px"
                    android:layout_marginRight="@dimen/dd_dimen_37px"
                    android:layout_marginTop="@dimen/dd_dimen_20px"
                    android:background="@color/common_CCCCCC" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/ll_layout10"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/dd_dimen_80px"
                android:layout_marginLeft="@dimen/dd_dimen_37px"
                android:layout_marginRight="@dimen/dd_dimen_37px"
                android:layout_marginTop="@dimen/dd_dimen_26px"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/tv_signed_name"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center_vertical"
                    android:gravity="center"
                    android:text="工作年限"
                    android:textColor="@color/common_text"
                    android:textSize="@dimen/dd_dimen_36px" />

                <EditText
                    android:id="@+id/et_signed_nianxian"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/dd_dimen_36px"
                    android:layout_toRightOf="@+id/tv_signed_name"
                    android:background="@null"
                    android:inputType="number"
                    android:singleLine="true"
                    android:textColor="@color/common_text"
                    android:textSize="@dimen/dd_dimen_36px" />
            </RelativeLayout>

            <View
                android:id="@+id/layout_line4"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/dd_dimen_1px"
                android:layout_below="@+id/ll_layout10"
                android:layout_marginLeft="@dimen/dd_dimen_37px"
                android:layout_marginRight="@dimen/dd_dimen_37px"
                android:layout_marginTop="@dimen/dd_dimen_20px"
                android:background="@color/common_CCCCCC" />

            <TextView
                android:id="@+id/tv_signed_next"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/dd_dimen_100px"
                android:layout_marginLeft="@dimen/dd_dimen_60px"
                android:layout_marginRight="@dimen/dd_dimen_60px"
                android:layout_marginTop="30dp"
                android:background="@drawable/shape_home_item_tv_sign_bg"
                android:gravity="center"
                android:text="完成"
                android:textColor="@color/common_white"
                android:textSize="@dimen/dd_dimen_36px" />
        </LinearLayout>
    </ScrollView>

    <RelativeLayout
        android:id="@+id/ll"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="vertical"
        android:visibility="gone" >
    </RelativeLayout>

</RelativeLayout>